import turtle as t
n=12
t.color("red")
t.begin_fill()
for x in range(n):
    t.forward(90)
    t.right(90)
    t.forward(90)
    t.right(90)
    t.forward(90)
    t.right(90)
    t.forward(90)
    t.right(360/6)
t.end_fill()

x=5
t.color("green")
t.begin_fill()
for x in range(x):
    t.circle(30)
    t.left(72)
t.end_fill()

t.color("black")
t.begin_fill()
t.circle(10)
t.end_fill()
